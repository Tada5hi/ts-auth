        "@vuelidate/validators": "^2.0.0",
        "cross-env": "^7.0.3",
        "ilingo": "^2.0.4",
        "rapiq": "^0.3.2",
        "rollup": "^3.1.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-vue": "^6.0.0-beta.11",
        "vue": "^3.2.41",
        "vue-tsc": "^1.0.9"
