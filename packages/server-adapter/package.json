{
    "name": "@authup/server-adapter",
    "version": "0.32.0",
    "description": "An adapter package to integrate into other services.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "test": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "npm run test -- --coverage"
    },
    "keywords": [
        "auth",
        "module",
        "typescript",
        "middleware",
        "http",
        "express",
        "socket",
        "socket.io",
        "websocket",
        "adapter",
        "oauth2"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/authup/authup.git",
        "directory": "packages/server-adapter"
    },
    "bugs": {
        "url": "https://github.com/authup/authup/issues"
    },
    "homepage": "https://github.com/authup/authup#readme",
    "dependencies": {
        "jose": "^4.13.1"
    },
    "devDependencies": {
        "@authup/core": "^0.32.0",
        "@authup/server-core": "^0.32.0",
        "@hapic/oauth2": "^1.6.1",
        "hapic": "^1.6.0",
        "redis-extension": "^1.3.0"
    },
    "peerDependencies": {
        "@authup/core": ">=0.32.0 <1.0.0",
        "@authup/server-core": ">=0.32.0 <1.0.0",
        "@hapic/oauth2": "^1.6.1",
        "hapic": "^1.6.0",
        "redis-extension": "^1.2.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
