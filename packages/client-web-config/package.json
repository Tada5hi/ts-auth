{
    "name": "@authup/client-web-config",
    "version": "1.0.1-beta.12",
    "description": "This package contains the web config module.",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "cross-env NODE_ENV=production rollup -c",
        "build": "rimraf ./dist && npm run build:js && npm run build:types",
        "build:watch": "npm run build -- --watch"
    },
    "keywords": [],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/authup/authup.git",
        "directory": "packages/client-web-config"
    },
    "bugs": {
        "url": "https://github.com/authup/authup/issues"
    },
    "homepage": "https://github.com/authup/authup#readme",
    "peerDependencies": {
        "@authup/kit": "^1.0.0-beta.13"
    },
    "devDependencies": {
        "@authup/kit": "^1.0.0-beta.13"
    },
    "dependencies": {
        "dycraft": "^1.1.0",
        "envix": "^1.5.0",
        "smob": "^1.5.0",
        "zod": "^3.23.7"
    },
    "publishConfig": {
        "access": "public"
    }
}
