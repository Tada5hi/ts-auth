{
    "name": "@authup/vue",
    "version": "0.10.0",
    "description": "This package contains vue components.",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "cross-env NODE_ENV=production rollup -c",
        "build": "rimraf ./dist && npm run build:js && npm run build:types",
        "build:watch": "npm run build -- --watch"
    },
    "keywords": [
        "auth",
        "module",
        "typescript",
        "template",
        "auth-template",
        "vue",
        "list",
        "form",
        "oauth2",
        "oauth2-client",
        "domains",
        "components"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tada5hi/authup.git",
        "directory": "packages/vue"
    },
    "bugs": {
        "url": "https://github.com/Tada5hi/authup/issues"
    },
    "homepage": "https://github.com/Tada5hi/authup#readme",
    "devDependencies": {
        "@authup/common": "^0.10.0",
        "@rollup/plugin-alias": "^4.0.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@types/node": "^18.11.18",
        "@vue-layout/hyperscript": "^0.1.2",
        "@vuelidate/core": "^2.0.0",
        "@vuelidate/validators": "^2.0.0",
        "cross-env": "^7.0.3",
        "esbuild": "^0.17.3",
        "ilingo": "^2.0.4",
        "rapiq": "^0.6.6",
        "rollup": "^3.10.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-vue": "^6.0.0-beta.11",
        "vue": "^3.2.45",
        "vue-tsc": "^1.0.24"
    },
    "dependencies": {
        "regenerator-runtime": "^0.13.10"
    },
    "peerDependencies": {
        "@authup/common": "^0.x",
        "@vue-layout/hyperscript": "^0.x",
        "@vuelidate/core": "^2.x",
        "@vuelidate/validators": "^2.x",
        "ilingo": "^2.x",
        "rapiq": ">=0.5.0 <1.0.0",
        "smob": "^0.x",
        "vue": "^3.x"
    },
    "publishConfig": {
        "access": "public"
    }
}
