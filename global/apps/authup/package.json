{
    "name": "authup",
    "version": "0.45.10",
    "description": "This is the CLI for the authup framework.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "cli": "node dist/index.cjs",
        "cli-dev": "npm run build && npm run cli",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "rimraf ./dist && cross-env NODE_ENV=production npm run build:js && npm run build:types"
    },
    "bin": {
        "authup": "dist/index.cjs"
    },
    "keywords": [],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://github.com/tada5hi"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/authup/authup.git",
        "directory": "global/apps/authup"
    },
    "bugs": {
        "url": "https://github.com/authup/authup/issues"
    },
    "homepage": "https://github.com/authup/authup#readme",
    "dependencies": {
        "@authup/client-web-app": "^0.45.10",
        "@authup/client-web-config": "^0.0.0",
        "@authup/config": "^0.0.0",
        "@authup/core": "^0.45.10",
        "@authup/server-core-app": "^0.45.10",
        "cac": "^6.7.14",
        "chalk": "^4.1.2",
        "consola": "^3.2.3",
        "dycraft": "^1.1.0",
        "resolve-package-path": "^4.0.3",
        "smob": "^1.4.0",
        "zod": "^3.22.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
